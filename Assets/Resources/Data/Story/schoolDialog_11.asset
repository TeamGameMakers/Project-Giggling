  sections:
  - text: "\u90A3\u662F\uFF0C\u90A3\u4E2A\u4EC0\u4E48\u6E38\u9F99\u9053\u58EB\u5417\u2026\u2026\uFF1F"
    leftSprite: {fileID: -7423440921397165710, guid: f861c22441f3ecd4b9cf4ac9b807f86f, type: 3}
    rightSprite: {fileID: 0}
    choices: []
  - text: "\u4ED6\u8981\u5BF9\u5C0F\u9759\u505A\u4EC0\u4E48\uFF1F\uFF01"
    leftSprite: {fileID: -7423440921397165710, guid: f861c22441f3ecd4b9cf4ac9b807f86f, type: 3}
    rightSprite: {fileID: 0}
    choices: []
  - text: "\u4F4F\u624B\uFF0C\u653E\u5F00\u5C0F\u9759\uFF01"
    leftSprite: {fileID: -7423440921397165710, guid: f861c22441f3ecd4b9cf4ac9b807f86f, type: 3}
    rightSprite: {fileID: 0}
    choices: []
  - text: "\u55EF\uFF1F"
    leftSprite: {fileID: 0}
    rightSprite: {fileID: 21300000, guid: bef956ff851dbe948962d5b6bf58bfe2, type: 3}
    choices: []
  - text: "\u4F60\uFF0C\u4F60\u80FD\u542C\u61C2\u6211\u8BF4\u7684\u8BDD\uFF1F"
    leftSprite: {fileID: -7423440921397165710, guid: f861c22441f3ecd4b9cf4ac9b807f86f, type: 3}
    rightSprite: {fileID: 0}
    choices: []
  - text: "\u554A\uFF0C\u80FD\u542C\u61C2\u3002"
    leftSprite: {fileID: 0}
    rightSprite: {fileID: 21300000, guid: bef956ff851dbe948962d5b6bf58bfe2, type: 3}
    choices: []
  - text: "\u4E0D\u8FC7\u4E5F\u771F\u591F\u7A00\u5947\u3002\u540C\u4E00\u5929\u3001\u540C\u4E00\u5730\u70B9\u5C45\u7136\u8BA9\u6211\u9047\u5230\u4E86\u4E24\u4E2A\u7075\u4F53\uFF0C\u96BE\u602A\u8FD9\u9644\u8FD1\u7684\u810F\u4E1C\u897F\u7A81\u7136\u591A\u4E86\u8D77\u6765\uFF0C\u539F\u6765\u90FD\u662F\u88AB\u4F60\u4EEC\u5438\u5F15\u7684\u3002"
    leftSprite: {fileID: 0}
    rightSprite: {fileID: 21300000, guid: bef956ff851dbe948962d5b6bf58bfe2, type: 3}
    choices: []
  - text: "\u4EC0\u4E48\uFF1F"
    leftSprite: {fileID: -7423440921397165710, guid: f861c22441f3ecd4b9cf4ac9b807f86f, type: 3}
    rightSprite: {fileID: 0}
    choices: []
  - text: "\u4F60\u4E0D\u4F1A\u641E\u4E0D\u6E05\u695A\u81EA\u5DF1\u7684\u72B6\u6001\u5427\uFF1F"
    leftSprite: {fileID: 0}
    rightSprite: {fileID: 21300000, guid: bef956ff851dbe948962d5b6bf58bfe2, type: 3}
    choices: []
  - text: "\u2026\u2026"
    leftSprite: {fileID: -7423440921397165710, guid: f861c22441f3ecd4b9cf4ac9b807f86f, type: 3}
    rightSprite: {fileID: 0}
    choices: []
  - text: "\u4F60\u5DF2\u7ECF\u6B7B\u4E86\u3002"
    leftSprite: {fileID: 0}
    rightSprite: {fileID: 21300000, guid: bef956ff851dbe948962d5b6bf58bfe2, type: 3}
    choices: []
  - text: "\u2026\u2026\u6B7B\u4E86\uFF1F"
    leftSprite: {fileID: -7423440921397165710, guid: f861c22441f3ecd4b9cf4ac9b807f86f, type: 3}
    rightSprite: {fileID: 0}
    choices: []
  - text: "\u662F\u7684\u3002\u4F60\u5DF2\u7ECF\u6B7B\u4E86\uFF0C\u4F46\u8FD8\u6CA1\u6709\u6B7B\u900F\u3002\u5C0F\u5C0F\u5E74\u7EAA\u5C45\u7136\u6709\u5982\u6B64\u6DF1\u7684\u6267\u5FF5\uFF0C\u4ECA\u513F\u53EF\u7B97\u662F\u89C1\u5230\u4E86\u3002\u542C\u597D\uFF0C\u4F60\u7684\u6267\u5FF5\u675F\u7F1A\u4E86\u4F60\u7684\u6B8B\u5B58\u7684\u7075\u9B42\uFF0C\u4F60\u4FBF\u4F1A\u6210\u4E3A\u7075\u4F53\u3002"
    leftSprite: {fileID: 0}
    rightSprite: {fileID: 21300000, guid: bef956ff851dbe948962d5b6bf58bfe2, type: 3}
    choices: []
  - text: "\u7075\u4F53\u53EA\u5177\u5907\u4E00\u4E9B\u7B80\u5355\u7684\u4EA4\u4E92\u80FD\u529B\uFF0C\u5E76\u4E14\u8FD9\u4E9B\u80FD\u529B\u4F1A\u968F\u7740\u65F6\u95F4\u7684\u63A8\u79FB\u800C\u6E10\u6E10\u6D88\u5931\u3002\u4F60\u5E94\u8BE5\u80FD\u591F\u611F\u53D7\u5230\u81EA\u5DF1\u8EAB\u4E0A\u7684\u5F02\u5E38\u5427\uFF1F\u73B0\u5728\u4F60\u624B\u4E0A\u62FF\u7740\u7684\u624B\u7535\u7B52\u4E0D\u89C9\u5F97\u6709\u4E9B\u91CD\u5417\uFF1F"
    leftSprite: {fileID: 0}
    rightSprite: {fileID: 21300000, guid: bef956ff851dbe948962d5b6bf58bfe2, type: 3}
    choices: []
  - text: "\u597D\u50CF\u2026\u2026\u662F\u6709\u70B9\u3002"
    leftSprite: {fileID: -7423440921397165710, guid: f861c22441f3ecd4b9cf4ac9b807f86f, type: 3}
    rightSprite: {fileID: 0}
    choices: []
  - text: "\u5475\u5475\uFF0C\u522B\u90A3\u4E48\u72D0\u7591\u561B\uFF0C\u5927\u53D4\u6211\u53EF\u6CA1\u6709\u9A97\u4F60\u54E6\u3002\u4F60\u73B0\u5728\u786E\u5B9E\u662F\u7075\u4F53\u72B6\u6001\u3002"
    leftSprite: {fileID: 0}
    rightSprite: {fileID: 21300000, guid: bef956ff851dbe948962d5b6bf58bfe2, type: 3}
    choices: []
  - text: "\u6211\u4E4B\u524D\u4E00\u76F4\u8BF4\u4E0D\u51FA\u8BDD\uFF0C\u73B0\u5728\u4E3A\u4EC0\u4E48\u2026\u2026"
    leftSprite: {fileID: -7423440921397165710, guid: f861c22441f3ecd4b9cf4ac9b807f86f, type: 3}
    rightSprite: {fileID: 0}
    choices: []
  - text: "\u7075\u4F53\u72B6\u6001\u4E0B\uFF0C\u53BB\u4E16\u524D\u8089\u4F53\u7684\u75C5\u75C7\u8868\u73B0\u5F97\u4F1A\u66F4\u52A0\u7A81\u51FA\u3002\u8BF4\u660E\u4F60\u6D3B\u7740\u7684\u65F6\u5019\u5C31\u4E0D\u592A\u80FD\u8BF4\u8BDD\uFF0C\u662F\u5417\uFF1F"
    leftSprite: {fileID: 0}
    rightSprite: {fileID: 21300000, guid: bef956ff851dbe948962d5b6bf58bfe2, type: 3}
    choices: []
  - text: "\u2026\u2026"
    leftSprite: {fileID: -7423440921397165710, guid: f861c22441f3ecd4b9cf4ac9b807f86f, type: 3}
    rightSprite: {fileID: 0}
    choices: []
  - text: "\u6211\u60F3\u8D77\u6765\u4E86\uFF0C\u4F60\u5C31\u662F\u90A3\u4E2A\u5C0F\u4F19\u5B50\u3002\u96BE\u602A\uFF0C\u8FD9\u4E2A\u5C0F\u59D1\u5A18\u2026\u2026"
    leftSprite: {fileID: 0}
    rightSprite: {fileID: 21300000, guid: bef956ff851dbe948962d5b6bf58bfe2, type: 3}
    choices: []
  - text: "\u54C8\u54C8\u54C8\uFF0C\u539F\u6765\u5982\u6B64\u554A\u3002"
    leftSprite: {fileID: 0}
    rightSprite: {fileID: 21300000, guid: bef956ff851dbe948962d5b6bf58bfe2, type: 3}
    choices: []
  - text: "\u6211\u5F97\u544A\u8BC9\u4F60\u4E00\u4E2A\u597D\u6D88\u606F\u3002"
    leftSprite: {fileID: 0}
    rightSprite: {fileID: 21300000, guid: bef956ff851dbe948962d5b6bf58bfe2, type: 3}
    choices: []
  - text: "\u4EC0\u4E48\uFF1F"
    leftSprite: {fileID: -7423440921397165710, guid: f861c22441f3ecd4b9cf4ac9b807f86f, type: 3}
    rightSprite: {fileID: 0}
    choices: []
  - text: "\u867D\u7136\u4F60\u786E\u5B9E\u5DF2\u7ECF\u6B7B\u4E86\uFF0C\u4F46\u8FD9\u4E2A\u5C0F\u59D1\u5A18\u2014\u2014\u8FD8\u6D3B\u7740\u3002\u5C31\u50CF\u65B0\u95FB\u91CC\u8BF4\u7684\u4E00\u6837\uFF0C\u5979\u8FD8\u5728\u91CD\u75C7\u76D1\u62A4\u5BA4\u91CC\uFF0C\u867D\u7136\u4E5F\u5FEB\u6B7B\u4E86\uFF0C\u4F46\u73B0\u5728\u81F3\u5C11\u8FD8\u6709\u6551\u3002"
    leftSprite: {fileID: 0}
    rightSprite: {fileID: 21300000, guid: bef956ff851dbe948962d5b6bf58bfe2, type: 3}
    choices: []
  - text: "\u6211\u4E0D\u77E5\u9053\u5979\u7684\u6267\u5FF5\u662F\u4EC0\u4E48\uFF0C\u4F46\u662F\u53EF\u4EE5\u80AF\u5B9A\u7684\u662F\uFF0C\u662F\u5979\u7684\u6267\u5FF5\u8BA9\u5979\u7684\u7075\u9B42\u6210\u4E3A\u4E86\u7075\u4F53\uFF0C\u73B0\u5728\u6B63\u6E38\u8361\u5728\u8089\u4F53\u4E4B\u5916\u3002\u5982\u679C\u4E0D\u6293\u7D27\u65F6\u95F4\u8BA9\u7075\u9B42\u56DE\u5F52\uFF0C\u6050\u6015\u4E0D\u4E45\u540E\u5C31\u4F1A\u5316\u6210\u90A3\u4E9B\u5C0F\u9B3C\u4E86\u3002"
    leftSprite: {fileID: 0}
    rightSprite: {fileID: 21300000, guid: bef956ff851dbe948962d5b6bf58bfe2, type: 3}
    choices: []
  - text: "\u5C0F\u9B3C\u2026\u2026\u4F60\u662F\u8BF4\uFF0C\u90A3\u4E9B\u9ED1\u8272\u7684\u602A\u7269\u5417\uFF1F"
    leftSprite: {fileID: -7423440921397165710, guid: f861c22441f3ecd4b9cf4ac9b807f86f, type: 3}
    rightSprite: {fileID: 0}
    choices: []
  - text: "\u7B54\u5BF9\u4E86\uFF01\u5176\u5B9E\u4F60\u73B0\u5728\u4E5F\u5728\u6162\u6162\u53D8\u6210\u90A3\u4E9B\u602A\u7269\u3002\u4F60\u4E5F\u5728\u5916\u9762\u6E38\u8361\u592A\u4E45\u4E86\u3002"
    leftSprite: {fileID: 0}
    rightSprite: {fileID: 21300000, guid: bef956ff851dbe948962d5b6bf58bfe2, type: 3}
    choices: []
  - text: "\u2026\u2026"
    leftSprite: {fileID: -7423440921397165710, guid: f861c22441f3ecd4b9cf4ac9b807f86f, type: 3}
    rightSprite: {fileID: 0}
    choices: []
  - text: "\u6240\u4EE5\uFF0C\u6211\u8BE5\u600E\u4E48\u505A\uFF1F"
    leftSprite: {fileID: -7423440921397165710, guid: f861c22441f3ecd4b9cf4ac9b807f86f, type: 3}
    rightSprite: {fileID: 0}
    choices: []
  - text: "\u505A\u4EC0\u4E48\uFF1F"
    leftSprite: {fileID: 0}
    rightSprite: {fileID: 21300000, guid: bef956ff851dbe948962d5b6bf58bfe2, type: 3}
    choices: []
  - text: "\u8FD8\u7528\u95EE\u5417\uFF1F\u5F53\u7136\u662F\u6551\u5C0F\u9759\u4E86\u2014\u2014\u4E0D\u7136\uFF0C\u4F60\u4E3A\u4EC0\u4E48\u8981\u8BF4\u4EC0\u4E48\u597D\u6D88\u606F\uFF1F"
    leftSprite: {fileID: -7423440921397165710, guid: f861c22441f3ecd4b9cf4ac9b807f86f, type: 3}
    rightSprite: {fileID: 0}
    choices: []
  - text: "\u54C8\u54C8\u54C8\u54C8\uFF0C\u806A\u660E\uFF01"
    leftSprite: {fileID: 0}
    rightSprite: {fileID: 21300000, guid: bef956ff851dbe948962d5b6bf58bfe2, type: 3}
    choices: []
  - text: "\u53EA\u6709\u4E00\u4E2A\u65B9\u6CD5\uFF1A"
    leftSprite: {fileID: 0}
    rightSprite: {fileID: 21300000, guid: bef956ff851dbe948962d5b6bf58bfe2, type: 3}
    choices: []
  - text: "\u7528\u7075\u9B42\u53BB\u8865\u5168\u7075\u9B42\u3002\u800C\u8FD9\u4E5F\u5C31\u610F\u5473\u7740\u2014\u2014\u9700\u8981\u727A\u7272\u4F60\uFF0C\u6765\u8865\u5168\u5979\u5FEB\u8981\u8FF7\u5931\u7684\u7075\u9B42\u3002"
    leftSprite: {fileID: 0}
    rightSprite: {fileID: 21300000, guid: bef956ff851dbe948962d5b6bf58bfe2, type: 3}
    choices: []
  - text: "\u4F60\uFF0C\u613F\u610F\u5417\uFF1F"
    leftSprite: {fileID: 0}
    rightSprite: {fileID: 21300000, guid: bef956ff851dbe948962d5b6bf58bfe2, type: 3}
    choices: []
